<%@page import="project.ConnectionProvider"%>
<%@page import="java.sql.*"%>

<%
String email=session.getAttribute("email").toString();
String address=request.getAttribute("address");
String city=request.getAttribute("city");
String state=request.getAttribute("state");
String country=request.getAttribute("email");
try{
	Connection con=ConnectionProvider.getCon();
	PreparedStatement ps=con.prepareStatement("update users set address=?, city=?, state=?,country=? where email=?");
	ps.setString(1,address);
	ps.setString(2,city);
	ps.setString(3,state);
	ps.setString(4,country);
	ps.setString(5,email);
	ps.executeQuery();
	response.sendRedirect("addChangeAddress.jsp?msg=valid");
}
catch(Exception e){
	System.out.println(e);
}
%>